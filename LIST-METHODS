jobs=['data analyst','data science',23,67]
jobs.append ('visualization')
print(jobs)
---------------
['data analyst', 'data science', 23, 67, 'visualization']


jobs=['data analyst','data science',23,67]
jobs.append (['visualization','power_bi',567])
print(jobs)
---------------------
['data analyst', 'data science', 23, 67, ['visualization', 'power_bi', 567]]


jobs=['data analyst','data science',23,67]
jobs.extend (['visualization','power_bi',567])
print(jobs)
------------------------
['data analyst', 'data science', 23, 67, 'visualization', 'power_bi', 567]



jobs=['data analyst','data science',23,67]
jobs.insert (3,'visualization')//3 indicates the index where do we need to insert
print(jobs)
----------------------
['data analyst', 'data science', 23, 'visualization', 67]


jobs=['data analyst','data science',23,67]
jobs.remove('data science')
print(jobs)
---------------------
['data analyst', 23, 67]


jobs=['data analyst','data science',23,67]
jobs.pop()
print(jobs)
--------------------
['data analyst', 'data science', 23]

jobs=[90,67,4,9,23,67]
jobs.sort()
print(jobs)
-------------------
[4, 9, 23, 67, 67, 90]


jobs=[90,67,4,9,23,67]
jobs.reverse()
print(jobs)
-----------------
[67, 23, 9, 4, 67, 90]



jobs=['data analyst','data science',23,67]
jobs.index('data science')
-----------------------
1
























