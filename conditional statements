salary=78000
if is_eligible and salary<50000:
    print("you're selected")
else:
    print("you're not selected ")//you're not selected 

is_eligible=True
salary=78000
if is_eligible or salary<50000:
    print("you're selected")
else:
    print("you're not selected ")//you're selected 

